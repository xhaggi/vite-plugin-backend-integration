{
  "name": "vite-plugin-backend-integration",
  "description": "Backend integration plugin for Vite",
  "version": "1.0.1",
  "author": "Sascha Woo",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
